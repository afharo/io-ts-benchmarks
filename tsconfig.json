{
  "compilerOptions": {
    "outDir": "./lib",
    "module": "commonjs",
    "lib": ["ES2015"],
    "strict": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "target": "es5",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./src"]
}
